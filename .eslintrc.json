{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": false
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "comma-dangle": "off",
        "semi": "off",
        "space-before-function-paren": "off"
    }
}